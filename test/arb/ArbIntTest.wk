module arb;

import wunit.Asserts;
import arb.ArbInt;
import arb.ArbIntProvider;

@TestClass
every ArbIntTest is:

	needs ArbIntProvider;

	#>	Depricated, included incase it's useful in the future
	#	Also, this should really be a helper class if implemented.
	@Test
	~[test aGetsLargestIntList]~(Asserts) {
		var ArbInt = ArbInt('0x0') from ArbIntProvider;
		var Int[] a = [];
		var Int[] b = [];
		ArbInt.aGetsLargestIntList([0,0],[0],a,b);
		Asserts.that(a.getSize() >= b.getSize());
		a = [];
		b = [];
		ArbInt.aGetsLargestIntList([0],[0,0],a,b);
		Asserts.that(a.getSize() >= b.getSize());
		a = [];
		b = [];
		ArbInt.aGetsLargestIntList([0,0],[0,0],a,b);
		Asserts.that(a.getSize() >= b.getSize());
		Asserts.that(a[0])Equals(0);
		Asserts.that(a[1])Equals(0);
		Asserts.that(b[0])Equals(0);
		Asserts.that(b[1])Equals(0);
	}
	##

	@Test
	testFromIntArray(Asserts) {
		var Int i = 0;
		var Int j = 0;
		var Int[] = [];
		for (i=0; i<20; i+=1) {
			Int[].push(i);
			var ArbInt a = ArbInt(Int[]) from ArbIntProvider;
			for (j=0; j<=i; j+=1) {
				Asserts.that(a.getWords()[j])Equals(j);
			}
		}
	}

	@Test
	testFromInt(Asserts) {
		var Int i = 0;
		for (i=9834; i<9854; i+=1) {
			var ArbInt a = ArbInt(i) from ArbIntProvider;
			Asserts.that(a.getWords()[0])Equals(i);
		}
	}

